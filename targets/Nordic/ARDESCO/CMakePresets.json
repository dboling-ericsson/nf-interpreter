{
    "version": 4,
    "include": [
        "../../../CMake/nordic.json",
        "../../../config/user-tools-repos.json",
        "../../../config/user-prefs.json"
    ],
    "configurePresets": [
        {
            "name": "ARDESCO",
            "inherits": [
                "nordic-preset",
                "user-tools-repos",
                "user-prefs"
            ],
            "hidden": false,
            "cacheVariables": {
                "TARGET_BOARD": "${presetName}",
                "TARGET_SERIES": "nrf9160",
                "USE_RNG": "OFF",
                "DP_FLOATINGPOINT": "OFF",
                "SUPPORT_ANY_BASE_CONVERSION": "OFF",
                "SWO_OUTPUT": "OFF",
                "NF_BUILD_RTM": "OFF",
                "NF_FEATURE_DEBUGGER": "ON",
                "NF_FEATURE_RTC": "OFF",
                "NF_FEATURE_HAS_CONFIG_BLOCK": "OFF",
                "NF_FEATURE_HAS_SDCARD": "OFF",
                "NF_FEATURE_HAS_USB_MSD": "OFF",
                "NF_NETWORKING_SNTP": "OFF",
                "API_nanoFramework.Device.Can": "OFF",
                "API_nanoFramework.Device.OneWire": "OFF",
                "API_System.Math": "ON",
                "API_System.Net": "OFF",
                "API_System.Device.Adc": "OFF",
                "API_Windows.Devices.Gpio": "ON",
                "API_System.Device.Gpio": "ON",
                "API_System.Device.I2c": "ON",
                "API_System.Device.Pwm": "OFF",
                "API_System.IO.Ports": "OFF",
                "API_System.Device.Spi": "OFF",
                "API_Windows.Storage": "OFF"
            }
        }
    ],
    "buildPresets": [
        {
            "inherits": "base-user",
            "name": "ARDESCO",
            "displayName": "ARDESCO",
            "configurePreset": "ARDESCO"
        }
    ]
}
